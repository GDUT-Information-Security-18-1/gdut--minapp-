page {
  background-color: #edece8;
}
.user_info_wrap {
  height: 45vh;
  overflow: hidden;
  background-color: var(--themeColor);
}
.user_info_wrap button {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.user_info_wrap .user_img_wrap {
  position: relative;
}
.user_info_wrap .user_img_wrap .user_bg {
  height: 50vh;
  width: 100%;
  filter: blur(7rpx);
}
.user_info_wrap .user_img_wrap .user_info {
  position: absolute;
  Left: 50%;
  transform: translateX(-50%);
  top: 20%;
  text-align: center;
}
.user_info_wrap .user_img_wrap .user_info .user_icon {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
.user_info_wrap .user_img_wrap .user_info .user_name {
  color: #fff;
  margin-top: 30rpx;
  font-size: 40rpx;
}
.user_content {
  position: relative;
}
.user_content .user_main {
  color: #666;
  padding-bottom: 100rpx;
  position: absolute;
  width: 90%;
  Left: 50%;
  transform: translateX(-50%);
  top: -58rpx;
}
.user_content .user_main .order_wrap {
  background-color: #fff;
  margin-top: 30rpx;
}
.user_content .user_main .order_wrap .order_title {
  padding: 20rpx;
  border-bottom: 1rpx solid #ccc;
}
.user_content .user_main .order_wrap .order_content {
  display: flex;
}
.user_content .user_main .order_wrap .order_content navigator {
  padding: 15rpx;
  flex: 1;
  text-align: center;
}
.user_content .user_main .order_wrap .order_content navigator .iconfont {
  height: 45rpx;
  color: var(--themeColor);
}
.user_content .user_main .address_wrap {
  margin-top: 30rpx;
  background-color: #fff;
  padding: 20rpx;
}
.user_content .user_main .app_info_wrap {
  margin-top: 30rpx;
  background-color: #fff;
}
.user_content .user_main .app_info_wrap .app_info_contact {
  display: flex;
  justify-content: space-between;
}
.user_content .user_main .app_info_wrap .app_info_item {
  padding: 20rpx;
  border-bottom: 1px solid #ccc;
}
