.OrderUserInfo {
  display: flex;
  height: 100rpx;
}
.OrderUserInfo .user_info_avatar image {
  width: 90rpx;
  height: 90rpx;
  margin: 20rpx;
  border-radius: 50%;
}
.OrderUserInfo .user_info_wrap {
  flex: 3;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.OrderUserInfo .user_info_wrap .user_name {
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.OrderUserInfo .user_info_wrap .user_time {
  color: var(--themeColor);
  font-size: 30rpx;
}
